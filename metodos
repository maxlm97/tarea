package tareauni1;

public class ejercicio1 {

	public static void main(String[] args) {
	multiplicar();
	}
	public static void multiplicar(){
	int a=4, b=8;
	int multiplicar= a*b;
	System.out.println("El resultado es:"+multiplicar );
	}
	}



