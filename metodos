package tareauni1;

public class Ejercicio15 {

	public static void main(String[] args) {
		int resul;
		resul = division (20,4);
		imprimir(resul);
		}
	public static int division(int x, int y){
		int divi=x/y;
		return divi;
	}
	public static void imprimir(int z){
		System.out.println("el reultado es: "+z);
	}

}






